{
  "extends": ["airbnb/base"],
  "rules": {
    "eslint-disable-next-line": 0,
    "class-methods-use-this": 0,
    "no-unused-vars": 1,
    "import/prefer-default-export": 0,
    "operator-linebreak": 0,
    "no-plusplus": 0,
    "linebreak-style": 0,
    "no-use-before-define": 0,
    "no-restricted-syntax": 0,
    "guard-for-in": 0,
    "object-curly-newline": 0,
    "comma-dangle": 0
  },
  "globals": {
    "document": true,
    "window": true
  },
  "parser": "babel-eslint",
  "env": {
    "jest": true
  }
}
